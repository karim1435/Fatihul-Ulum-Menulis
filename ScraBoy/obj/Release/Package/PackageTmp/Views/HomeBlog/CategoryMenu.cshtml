@model IEnumerable<string>

<nav class="navbar navbar-default">
    <li style="list-style-type: none">
        <a style="font-size:20PX;" class="text-white" href="#" id="navbarDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            <i style="color:white" class="fa fa-search"> Explore</i>
        </a>
        <ul class="dropdown-menu dropdown-menu-left notify-drop">
            <div class="drop-content">
                @foreach(var item in Model)
                {
                    <li>

                        <div class="col-md-10 col-sm-9 col-xs-9 pd-l0">
                            <a style="font-size:20px" class="dropdown-item" href="@Url.Action("Category",new { catId = item })">@item</a>
                        </div>
                    </li>
                }
            </div>

        </ul>
    </li>
</nav>